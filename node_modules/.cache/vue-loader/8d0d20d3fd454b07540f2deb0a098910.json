    addTodo(todo){\n      this.todos.push(todo);\n      this.copyTodos = [...this.todos];\n    },\n    querySearch(query){\n      if(query.trim() === ''){\n        this.copyTodos = [...this.todos];\n      }else{\n        const temp = this.todos.filter(todo =>{\n          return todo.title.includes(query)\n        });\n\n        this.copyTodos = [...temp];\n      }\n    }\n  },\n  data(){\n    return {\n      todos: [\n        {\n          id: 0,\n          title:  'Tarea pendiente',\n          completed: false\n        },\n        {\n          id: 1,\n          title:  'Tarea completada',\n          completed: true\n        }\n      ],\n      copyTodos: []\n    }\n  },\n  created(){\n    this.copyTodos = [...this.todos];\n  }\n}\n</script>\n\n<style>\n  *{\n    box-sizing: border-box;\n  }\n\n  body{\n    font-family: Arial, Helvetica, sans-serif;\n    font-size: 1.5em;\n    padding: 0;\n    margin: 0;\n  }\n\n  #main-container{\n    border: solid 1px #ccc;\n    width: auto;\n    margin: auto;\n  }\n\n  #header{\n    background: black;\n    padding: 10px;\n  }\n\n  h2{\n    padding: 0 10px;\n  }\n  #title{\n    text-align: center;\n    color: purple;\n  }\n</style>\n"]}]}